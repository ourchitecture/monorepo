ARG NEXUS_VERSION=3.66.0

ARG IMAGE_REGISTRY=docker.io/
ARG IMAGE_NAME=sonatype/nexus3
ARG IMAGE_SHA=sha256:c63b3fb721e9c0339a721a911a544098a2d3ba03f74dd792fce08caf04b2326f
ARG IMAGE_TAG=${NEXUS_VERSION}@${IMAGE_SHA}
ARG BASE_IMAGE=${IMAGE_REGISTRY}${IMAGE_NAME}:${IMAGE_TAG}

FROM ${BASE_IMAGE} as base
